{
  "expo": {
    "name": "foodieByChloe",
    "slug": "foodieByChloe",
    "scheme": "foodiebychloe",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EEECE3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foodiebychloe",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "foodieByChloe拍攝美食照片來加到您的帖文中",
        "NSPhotoLibraryUsageDescription": "從圖片庫上載美食照片來加到您的帖文中",
        "NSPhotoLibraryAddUsageDescription": "foodieByChloe下載美食照片到您的圖庫中",
        "NSLocationWhenInUseUsageDescription": "foodieByChloe以位置搜尋附近餐廳",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      },
      "buildNumber": "39"
    },
    "android": {
      "package": "com.foodiebychloe",
      "versionCode": 39,
      "permissions": [
        "CAMERA_ROLL",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#EEECE3"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBParPaZC61CNm5ouh-4vt9aloKsmTVCCQ"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "sourceExts": [
        "cjs"
      ]
    }
  }
}
